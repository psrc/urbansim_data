  <model_manager>
    <models config_name="model_system" hidden="False" name="Models" setexpanded="True" type="dictionary">
      <model name="external_demographic_model_hh" type="model">
        <structure type="dictionary">
          <import name="import" type="dictionary">
            <class_module type="string">zurich_parcel.models.external_demographic_model</class_module>
            <class_name type="string">ExternalDemographicModel</class_name>
          </import>
          <init type="dictionary">
            <name name="name" type="string">ExternalDemographicModel</name>
          </init>
          <run type="dictionary">
            <argument name="demographic_data_file" parser_action="quote_string" type="string">demography.hdf5</argument>
            <argument model_dependency_type="dataset" name="household_dataset" type="string">household</argument>
            <argument model_dependency_type="dataset" name="person_dataset" type="string">person</argument>
            <argument name="keep_attributes" type="string">['building_id', 'cars', 'income', 'living_unit_id', 'race_id', 'x_coord_ha', 'y_coord_ha', 'zgde','household_vz_id']</argument>
            <argument name="keep_attributes_p" type="string">['job_id']</argument>
            <argument name="fill_value" type="string">-1</argument>
            <argument name="demographic_attributes" type="dictionary">
              <attribute name="household_id" type="string">household.household_id</attribute>
              <attribute name="age_of_head" type="string">household.aggregate(person.age * paris.person.is_head_of_household)</attribute>
              <attribute name="children" type="string">household.aggregate(person.age &lt; 16)</attribute>
              <attribute name="persons" type="string">household.number_of_agents(person)</attribute>
              <attribute name="workers" type="string">household.aggregate(person.work_stat==1)</attribute>
              <attribute name="race_id" type="string">household.aggregate(numpy.ma.masked_where(person.id == houshold.ref_per_id, person.place_of_birth))</attribute>
            </argument>
            <argument name="demographic_attributes_p" type="dictionary">
              <attribute name="person_id" type="string">person.id</attribute>
              <attribute name="work_at_home" type="string">0</attribute>
              <attribute name="age" type="string">person.age</attribute>
              <attribute name="household_id" type="string">person.hh_id</attribute>
              <attribute name="sex" type="string">person.sex</attribute>
              <attribute name="worker" type="string">person.work_stat</attribute>
              <attribute name="partner_id" type="string">person.partner_id</attribute>
              <attribute name="workstat" type="string">person.work_stat</attribute>
            </argument>
          </run>
        </structure>
      </model>
    </models>
  </model_manager>
