    <parent type="file">zurich_parcel/configs/zurich_kt_parcel.xml</parent>
    <expression_library type="dictionary">
      <variable name="household.sc_dist_to_station_hh" source="expression" type="variable_definition" use="both">household.disaggregate(parcel.dist_to_station, intermediates=[building,living_unit])</variable>
      <variable name="household.sc_dist_to_zh_cbd_hh" source="expression" type="variable_definition" use="both">household.disaggregate(parcel.dist_to_zh_cbd, intermediates=[building,living_unit])</variable>
      <variable name="living_unit_x_household.sc_rent_price_to_income_lu" source="expression" type="variable_definition" use="both">((exp(living_unit.ln_rent_price) &gt; 0)* exp(living_unit.ln_rent_price))/(household.income * 1.0) </variable>
    </expression_library>
  </general>
