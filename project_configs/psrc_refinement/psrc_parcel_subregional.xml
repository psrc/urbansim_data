      <model name="household_transition_model" type="model">
        <structure type="dictionary">
          <run type="dictionary">
            <argument name="sampling_threshold" type="string" parser_action="quote_string" model_dependency_type="variable">safe_array_divide(control_total.number_of_agents(household), control_total.total_number_of_households) &gt; 0.33</argument>
            <argument name="sampling_hierarchy" type="string">['city_id', 'county_id']</argument>
         </run>
        </structure>
      </model>
